{
  "openapi": "3.0.0",
  "info": {
    "title": "ts-app",
    "version": "1.0.0",
    "description": "Specification JSONs: [v2](/api-spec/v2), [v3](/api-spec/v3)."
  },
  "paths": {
    "/api/": {
      "get": {
        "summary": "/api/",
        "responses": {},
        "tags": []
      }
    },
    "/api/signup": {
      "post": {
        "summary": "/api/signup",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "name",
                  "photoUrls"
                ],
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "vooer@test.ro"
                  },
                  "hashedPassword": {
                    "type": "string",
                    "example": "string"
                  }
                }
              }
            }
          },
          "description": "Pet object that needs to be added to the store"
        },
        "responses": {},
        "tags": []
      }
    },
    "/api/signin": {
      "post": {
        "summary": "/api/signin",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "vasya2@ez.com"
                  },
                  "hashedPassword": {
                    "type": "string",
                    "example": "vasya2@ez.com"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {},
        "tags": []
      }
    },
    "/api/info": {
      "get": {
        "summary": "/api/info",
        "responses": {
          "401": {
            "description": "Unauthorized"
          }
        },
        "tags": [],
        "security": [
          {
            "authorization": []
          }
        ]
      }
    },
    "/api/user/{id}": {
      "get": {
        "summary": "/api/user/{id}",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "example": 1,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {},
        "tags": []
      }
    },
    "/api/users": {
      "get": {
        "summary": "/api/users",
        "responses": {},
        "tags": []
      }
    }
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "components": {
    "securitySchemes": {
      "authorization": {
        "name": "authorization",
        "in": "header",
        "type": "apiKey"
      }
    }
  }
}